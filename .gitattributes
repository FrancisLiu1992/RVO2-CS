[attr]whitespace-2  whitespace=tab-in-indent,tabwidth=2,trailing-space
[attr]whitespace-4  whitespace=tab-in-indent,tabwidth=4,trailing-space

*               text=auto

*.cmake         text whitespace-2
*.cs            text diff=csharp whitespace-4
*.csproj        text diff=html whitespace-2
*.css           text diff=css whitespace=trailing-space
*.html          text diff=html whitespace=trailing-space
*.md            text diff=markdown whitespace=-blank-at-eol,blank-at-eof,tab-in-indent
*.sln           text text whitespace=tabwidth=2,trailing-space
*.yml           text whitespace-2

.editorconfig   text whitespace-2
.gitattributes  text whitespace-2
.gitignore      text whitespace-2
CMakeLists.txt  text whitespace-2
Doxyfile        text whitespace-2
LICENSE         text whitespace=trailing-space

.gitattributes  export-ignore
.gitignore      export-ignore

.github/**      export-ignore
